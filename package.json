{
  "name": "file-uploader",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview --mode=test",
    "dev:dev": "electron-vite dev --mode=dev",
    "dev:test": "electron-vite dev --mode=test",
    "dev:prod": "electron-vite dev --mode=prod",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win:dev": "pnpm run build --mode=dev && electron-builder --win --config",
    "build:win:test": "pnpm run build --mode=test && electron-builder --win --config",
    "build:win:prod": "pnpm run build --mode=prod && electron-builder --win --config",
    "build:mac:dev": "pnpm run build --mode=dev && electron-builder --mac --config",
    "build:mac:test": "pnpm run build --mode=test && electron-builder --mac --config",
    "build:mac:prod": "pnpm run build --mode=prod && electron-builder --mac --config",
    "build:linux:prod": "pnpm run build --mode=prod && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "axios": "^1.4.0",
    "electron-updater": "^5.3.0",
    "element-plus": "^2.3.8",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue-router": "4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.3.2",
    "@swc/core": "^1.3.72",
    "@types/node": "^18.16.19",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "electron": "^24.6.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "less": "^4.1.3",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "typescript": "^5.1.6",
    "unocss": "^0.54.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.2",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.4"
  }
}
